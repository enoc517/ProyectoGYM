@model IEnumerable<ProyectoGYM.Models.Actividade>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdsalaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdsalaNavigation.Idsala)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idactividades">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idactividades">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idactividades">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
