@model IEnumerable<ProyectoGYM.Models.Cliente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellidos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dirrecion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNacimento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cedula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SesionesRayosUva)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdclienteMembresiaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellidos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dirrecion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaNacimento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SesionesRayosUva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdclienteMembresiaNavigation.IdclienteMembresia)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idcliente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idcliente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idcliente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
